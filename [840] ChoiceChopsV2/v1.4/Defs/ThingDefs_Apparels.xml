<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    	<!-- Raider Leather Jacket Kobra-->

  <ThingDef ParentName="ApparelMakeableBase">
    <defName>MORG_RLJK</defName>
    <label>Raider Jacket Cobra</label>
    <description>Your basic jacket modified with crude shoulderpads and spikes.</description>
    <graphicData>
      <texPath>Apparel/Kobra/RLJK</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  		<drawSize>1.0</drawSize>
			<!--<drawOffsetNorth>(0,0.01,0)</drawOffsetNorth>
			<drawOffsetEast>(0,0.01,0)</drawOffsetEast>
			<drawOffsetSouth>(0,0,0)</drawOffsetSouth>
			<drawOffsetWest>(0,0.01,0)</drawOffsetWest>-->
    </graphicData>
		<modExtensions>
			<!--<li Class="OversizedApparel.Extension">
				<drawSize>(1, 1)</drawSize>
			</li>-->
		</modExtensions>
	<costList>
		<Steel>15</Steel>
	</costList>
     <costStuffCount>55</costStuffCount>
    <stuffCategories>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
	  <Mass>4</Mass>
      <MaxHitPoints>280</MaxHitPoints>
      <WorkToMake>12000</WorkToMake>
	<StuffEffectMultiplierInsulation_Heat>0.45</StuffEffectMultiplierInsulation_Heat>
	<StuffEffectMultiplierInsulation_Cold>0.75</StuffEffectMultiplierInsulation_Cold>
    <EquipDelay>1.5</EquipDelay>
	<StuffEffectMultiplierArmor>0.7</StuffEffectMultiplierArmor>
    </statBases>
    <equippedStatOffsets>
	  <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">0.3</SlaveSuppressionOffset>
	  <MoveSpeed>-0.06</MoveSpeed>
	  <PainShockThreshold>0.15</PainShockThreshold>
    </equippedStatOffsets>
	<generateCommonality>0.1</generateCommonality>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Kobra/RLJK</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
	  <li>postapo</li>
      </tags>
      <defaultOutfitTags>
	  <li>postapo</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
  
