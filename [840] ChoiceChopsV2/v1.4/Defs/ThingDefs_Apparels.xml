    <!--Raider Shoulderpads Armour-->
  
    <ThingDef ParentName="ApparelMakeableBase">
    <defName>MORG_RSA</defName>
    <label>Raider Shoulderpads</label>
    <description>An old world football shoulderpads set that protect the wearer's shoulders and body, raiders like to add some spikey bits on the shoulders.</description>
    <techLevel>Neolithic</techLevel>
    <graphicData>
      <texPath>Apparel/Shoulder/RSA</texPath>
		<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1.0</drawSize>
			<!--<drawOffsetNorth>(0,0.01,0)</drawOffsetNorth>
			<drawOffsetEast>(0,0.01,0)</drawOffsetEast>
			<drawOffsetSouth>(0,0,0)</drawOffsetSouth>
			<drawOffsetWest>(0,0.01,0)</drawOffsetWest>-->
	</graphicData>
			<modExtensions>
				<!--<li Class="OversizedApparel.Extension">
					<drawSize>(1, 1)</drawSize>
				</li>-->
			</modExtensions>
    <costList>
        <Leather_Patch>50</Leather_Patch>
		<Steel>15</Steel>
    </costList>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <WorkToMake>5000</WorkToMake>
      <Mass>4</Mass>
      <ArmorRating_Sharp>0.30</ArmorRating_Sharp>
	  <ArmorRating_Blunt>0.85</ArmorRating_Blunt>
	  <ArmorRating_Heat>0.1</ArmorRating_Heat>
	  <Insulation_Cold>3</Insulation_Cold>
	  <Insulation_Heat>3</Insulation_Heat>
      <EquipDelay>3.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.2</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Shoulders</li>
        <li>Torso</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Shoulder/RSA</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
  
     <!-- Plunderer Pants -->

  <ThingDef ParentName="ApparelMakeableBase">
    <defName>MORG_PP</defName>
    <label>Raider Plunderer Pants</label>
    <description>A dummy text.</description>
    <graphicData>
      <texPath>Apparel/Plunderer/PP</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
        <costList>
            <Leather_Patch>25</Leather_Patch>
        </costList>
    <statBases>
      <WorkToMake>4000</WorkToMake>
