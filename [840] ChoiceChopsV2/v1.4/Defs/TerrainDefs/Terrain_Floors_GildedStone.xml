<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- ================= First Subdivision: Type ================= -->

  <TerrainDef Name="JFGeometricFloors_GildedStone" Abstract="True">
    <description>Gilded stone tile with complex patterns. A perfect choice for satisfying your ego, or perhaps vanity.</description>
    <layerable>true</layerable>
    <affordances>
      <li>Light</li>
      <li>Medium</li>
      <li>Heavy</li>
    </affordances>
    <tags>
      <li>Floor</li>
      <li>FineFloor</li>
    </tags>
    <statBases>
      <Beauty>4</Beauty>
      <WorkToBuild>2000</WorkToBuild>
      <CleaningTimeFactor>0.8</CleaningTimeFactor>
    </statBases>
    <fertility>0</fertility>
    <designationCategory>Floors</designationCategory>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <edgeType>Hard</edgeType>
    <constructEffect>ConstructDirt</constructEffect>
    <researchPrerequisites><li>Stonecutting</li></researchPrerequisites>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <resourcesFractionWhenDeconstructed>0.5</resourcesFractionWhenDeconstructed>
    <pollutionShaderType MayRequire="Ludeon.RimWorld.Biotech">TerrainFadeRoughLinearBurn</pollutionShaderType>
    <pollutionOverlayTexturePath>Terrain/Surfaces/PollutionFloorSmooth</pollutionOverlayTexturePath>
    <pollutionColor>(1, 1, 1, 0.8)</pollutionColor>
    <pollutionOverlayScale>(0.75, 0.75)</pollutionOverlayScale>
    <pollutionTintColor>(0.95, 0.95, 0.93, 1)</pollutionTintColor>
    <renderPrecedence>200</renderPrecedence>
    <uiOrder>2116</uiOrder>
  </TerrainDef>

