<Defs>

  <!-- ================= First Subdivision: Base ================= -->

  <ThingDef Abstract="True" Name="JFBottomLayer" ParentName="BuildingBase">
    <altitudeLayer>Floor</altitudeLayer>
    <blockPlants>true</blockPlants>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsFurniture</li>
    </thingCategories>
    <designationCategory>Floors</designationCategory>
    <selectable>true</selectable>
    <useHitPoints>true</useHitPoints>
    <defaultPlacingRot>North</defaultPlacingRot>
    <building>
      <isEdifice>false</isEdifice>
      <paintable>true</paintable>
    </building>
    <graphicData>
      <shaderType>CutoutComplex</shaderType>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <blocksAltitudes>
      <li>Floor</li>
    </blocksAltitudes>
    <researchPrerequisites><li>CarpetMaking</li></researchPrerequisites>  
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <uiOrder>2117</uiOrder>
  </ThingDef>
  
  <!-- ================= Second Subdivision: Pattern ================= -->

  <ThingDef Abstract="True" Name="JFBottomLayer_CircleA" ParentName="JFBottomLayer">
    <description>Circle bottom layer A.</description>
    <graphicData>
      <texPath>FloorCoverings/BottomLayer/CircleA</texPath>
      <color>(190, 184, 175)</color>
    </graphicData>
    <uiIconPath>Icons/BottomLayer/CircleA</uiIconPath>
  </ThingDef>

